@{

}
<!DOCTYPE html>
<html>
<head>
    <title> Person of the Year</title>
    <link rel="stylesheet" href="~/styles.css" />
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Year</th>
                <th>Name</th>
                <th>Honor</th>
                <th>Country</th>
                <th>Date of Birth</th>
                <th>Death</th>
                <th>Title</th>
                <th>Category</th>
                <th>What They Did:</th>
            </tr>
        </thead>
        @foreach (var person in Model)
        {
            <tr>
                <td>@person.Year</td>
                <td>@person.Honor</td>
                <td>@person.Name</td>
                <td>@person.Country</td>
                <td>@person.BirthYear</td>
                <td>@person.DeathYear</td>
                <td>@person.Title</td>
                <td>@person.Category</td>
                <td>@person.Context</td>
            </tr>
        }
    </table>
</body>

</html>